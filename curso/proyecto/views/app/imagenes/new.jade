extends ../../layout.jade
//identificador
block contenido
    div(class="login col-md-6 remove-float center-block  big-top-space")
        h1 Subir nueva Imagen
        form(action="/app/imagenes", method="POST")
                div(class="form-group")
                    label(for="title") Títutlo
                    input(type="text", name="title" placeholder="Títutlo de la imagen" id="title" class="form-control")
                    div
                    label(for="archivo") Imagen
                    input(type="file", name="archivo", id="archivo" class="form-control")
                    div(class="top-space")
                        input(type="submit", value="Iniciar Sesión" class="btn btn-info btn-block")
    
            